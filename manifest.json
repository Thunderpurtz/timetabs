{
  "name": "TimeTabs",
  "version": "0.1.0",
  "description": "Automatically sorts tabs based on last open",
  "author": "Austin Zhang, Mason Qian",
  "manifest_version": 2,
  "short_name": "TimeTabs",
  "permissions":["storage"],
  "background": {
    "scripts": [
      "rearrange.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
